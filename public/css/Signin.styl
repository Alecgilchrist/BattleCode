@import './imports'

.Signin
  text-align center

.signin-buttons
  display flex
  align-content center

.login-btn
  background-color #FFFFFF
  border 2px solid #e7e7e7
  padding 15px 32px
  text-align center
  font-weight bold
  font-size 100%
  transition-duration 0.8s
  width 50%

.login-btn:hover
  background-color #e7e7e7